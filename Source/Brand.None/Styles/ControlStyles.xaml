<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--
        This is the unbranded version of the application, i.e. the version that only uses the
        fallback styles.
    -->
    
    <!-- Merge with fallback styles -->
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/Brand.Fallback;component/Styles/ControlStyles.xaml" />
    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>